{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "scope": "supervan-tech",
  "github": {
    "enabled": false
  },
  "buildCommand": "cd apps/optimiz; pnpm run openapi; pnpm run build",
  "outputDirectory": "./apps/optimiz/dist",
  "redirects": [],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
